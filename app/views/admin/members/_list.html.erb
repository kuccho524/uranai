<table class='table table-hover table-inverse'>
  <thead>
    <th>氏名</th>
    <th>メールアドレス</th>
  </thead>
  <tbody>
    <% members.each do |member| %>
      <tr>
        <td>
          <%= link_to admin_member_path(member.id), method: :get, local:true do %>
            <%= member.last_name %>　<%= member.first_name %>
          <% end %>
        </td>
        <td>
          <%= member.email %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>